{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/node_modules/**"
    ],
    "trailingSlash": true,
    "redirects": [
      {
        "source": "/contacts/",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/depy",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/depy/depy",
        "destination": "/depy",
        "type": 301
      },
      {
        "source": "/bootstrap",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/bootstrap/bootstrap-font-sizes",
        "destination": "/bootstrap-font-sizes",
        "type": 301
      },
      {
        "source": "/servers",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/servers/nginx-geoip2-ubuntu",
        "destination": "/nginx-geoip2",
        "type": 301
      },
      {
        "source": "/laravel/:uri*",
        "destination": "/:uri",
        "type": 301
      },
      {
        "source": "/tools/:uri*",
        "destination": "/:uri",
        "type": 301
      },
      {
        "source": "/:page*/index.html",
        "destination": "/:page",
        "type": 301
      },
      {
        "source": "/:page*/index",
        "destination": "/:page",
        "type": 301
      }
    ]
  }
}
